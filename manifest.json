{
  "manifest_version": 2,

  "name": "Scotiabank Extension",
  "description": "Banking help extension",
  "version": "1.0.0",
  "icons": { "128": "icon_128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "content.html"
  },
  "permissions": ["activeTab"],
  "content scripts": [
    {
      "js": ["scripts/popup.js"]
    }
  ]
}
